 <root main_tree_to_execute = "GateTaskTree" >
    <BehaviorTree ID="GateTaskTree">
        <Sequence>
            <AlwaysSuccess>
                <Timeout msec="10000">
                    <NavigateToPose server_name="{server_name}" goal="0;0;0;0" depth_lock="standard_depth" />
                </Timeout>
            </AlwaysSuccess>

            <NavigateToPose server_name="{server_name}" goal="10;0" yaw_lock="{fixed_yaw}" depth_lock="{standard_depth}"/>
            <NavigateToPose server_name="{server_name}" goal="0;-1" yaw_lock="{fixed_yaw}" depth_lock="{standard_depth}"/>
            <NavigateToPose server_name="{server_name}" goal="2;0" yaw_lock="{fixed_yaw}" depth_lock="{standard_depth}"/>

            <SubTree ID="DanceTree" node="node" server_name="server_name" standard_depth="standard_depth" />

            <NavigateToPose server_name="{server_name}" goal="0;0" yaw_lock="{fixed_yaw}" depth_lock="{standard_depth}" yaw_tolerance="1" />

        </Sequence>
    </BehaviorTree>

    <BehaviorTree ID="DanceTree">
        <Repeat num_cycles="4">
            <Timeout msec="6000">
                <NavigateToPose server_name="{server_name}" goal="0;0;0;90" depth_lock="{standard_depth}" yaw_tolerance="10"/>
            </Timeout>
        </Repeat>
    </BehaviorTree>
</root>